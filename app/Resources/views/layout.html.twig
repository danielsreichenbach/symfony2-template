{% extends 'base.html.twig' %}

{% block body %}
  <div>
{{ knp_menu_render('user') }}
  </div>

  {% if app.request.hasPreviousSession %}
    {% for type, messages in app.session.flashbag.all() %}
      {% for message in messages %}
        <div class="flash-{{ type }}">
          {{ message }}
        </div>
      {% endfor %}
    {% endfor %}
  {% endif %}
{% block content %}{% endblock content %}
{% endblock body %}
